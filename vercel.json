{
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install && npm install --prefix client",
  "rewrites": [
    {
      "source": "/api/bybit/(.*)",
      "destination": "https://api.bybit.com/v5/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
